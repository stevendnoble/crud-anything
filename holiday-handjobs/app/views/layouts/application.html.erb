<!DOCTYPE html>
<html>
<head>
  <title>HolidayHandjobs</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= yield :javascripts %>
  <%= csrf_meta_tags %>
</head>
<body>

<br>
<br>
<div class="container">
	<div class="row">
		<div class="col-xs-12 col-lg-10 col-lg-offset-1 col-xl-8 col-xl-offset-2">
			<div class="stitch">
			<!-- Navbar -->
			<nav class="navbar navbar-inverse">
			  <div class="container-fluid">
			    <!-- Brand and toggle get grouped for better mobile display -->
			    <div class="navbar-header">
			      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#holiday-handjobs-collapse" aria-expanded="false">
			        <span class="sr-only">Toggle navigation</span>
			        <span class="icon-bar"></span>
			        <span class="icon-bar"></span>
			        <span class="icon-bar"></span>
			      </button>
			      <a class="navbar-brand" href="/">Holiday Handjobs</a>
			    </div>
			    <div class="collapse navbar-collapse" id="holiday-handjobs-collapse">
			      <ul class="nav navbar-nav">
			        <li><a href="/about">about</a></li>
			        <li><a href="/vendors">vendors</a></li>
			        <li><a href="/location">where & when</a></li>
			        <li><a href="/media">media</a></li>
			        <li><a href="/contact">contact</a></li>
			      </ul>
			      <ul class="nav navbar-nav navbar-right">
			        <li><a href="/signup">vendor signup</a></li>
			        <li><a href="/login">vendor login</a></li>
			      </ul>
			    </div><!-- /.navbar-collapse -->
			  </div><!-- /.container-fluid -->
			</nav>
			<%= yield %>
		</div>
	</div>
</div>
</div>

</body>
</html>
